#Bashtop theme with flat-remix colors
#by Daniel Ruiz de Alegr√≠a <daniel@drasite.com>

# Colors should be in 6 or 2 character hexadecimal or single spaced rgb decimal: "#RRGGBB", "#BW" or "0-255 0-255 0-255"
# example for white: "#ffffff", "#ff" or "255 255 255".

# All graphs and meters can be gradients
# For single color graphs leave "mid" and "end" variable empty.
# Use "start" and "end" variables for two color gradient
# Use "start", "mid" and "end" for three color gradient

# Main background, empty for terminal default, need to be empty if you want transparent background
theme[main_bg]=""

# Main text color
theme[main_fg]="#d3d9dd"

# Title color for boxes
theme[title]="#d3d9dd"

# Higlight color for keyboard shortcuts
theme[hi_fg]="#7db2de"

# Background color of selected item in processes box
theme[selected_bg]="#7db2de"

# Foreground color of selected item in processes box
theme[selected_fg]="#d3d9dd"

# Color of inactive/disabled text
theme[inactive_fg]="#83b1e0"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#e4b995"

# Cpu box outline color
theme[cpu_box]="#93979a"

# Memory/disks box outline color
theme[mem_box]="#93979a"

# Net up/down box outline color
theme[net_box]="#93979a"

# Processes box outline color
theme[proc_box]="#93979a"

# Box divider line and small boxes line color
theme[div_line]="#93979a"

# Temperature graph colors
theme[temp_start]="#7db2de"
theme[temp_mid]="#8fbce3"
theme[temp_end]="#d3d9dd"

# CPU graph colors
theme[cpu_start]="#7db2de"
theme[cpu_mid]="#8fbce3"
theme[cpu_end]="#d3d9dd"

# Mem/Disk free meter
theme[free_start]="#7db2de"
theme[free_mid]="#8fbce3"
theme[free_end]="#d3d9dd"

# Mem/Disk cached meter
theme[cached_start]="#7db2de"
theme[cached_mid]="#8fbce3"
theme[cached_end]="#d3d9dd"

# Mem/Disk available meter
theme[available_start]="#7db2de"
theme[available_mid]="#8fbce3"
theme[available_end]="#d3d9dd"

# Mem/Disk used meter
theme[used_start]="#7db2de"
theme[used_mid]="#8fbce3"
theme[used_end]="#d3d9dd"

# Download graph colors
theme[download_start]="#7db2de"
theme[download_mid]="#8fbce3"
theme[download_end]="#d3d9dd"

# Upload graph colors
theme[upload_start]="#7db2de"
theme[upload_mid]="#8fbce3"
theme[upload_end]="#d3d9dd"
